# ALLIANCE_GOOGLE_CLOUD_SQL_DATABASE=
# ALLIANCE_GOOGLE_CLOUD_SQL_USER=
# ALLIANCE_GOOGLE_CLOUD_SQL_PASSWORD=

# Simple authentication token for the /private/** endpoints
SOURCIFY_PRIVATE_TOKEN=

# UI session secret
